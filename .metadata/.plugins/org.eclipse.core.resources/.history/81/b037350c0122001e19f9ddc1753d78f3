package datastructures;

public class BubbleSort {
	public static void main(String[] args) {
		
		int arr[]= {1,9,2,5,7,8,3,4,5,8,9,10,23,12,24,15};
		int n=arr.length;
		System.out.println(n);
		System.out.println(arr[15]);
	int temp;
	
		for(int i=0;i<n;i++) {
			for(int j=i+1;j<n;j++) {
				if(arr[i]<arr[j]) {
					System.out.print(arr[i]);
				}
			}
			
		}
	}

}
